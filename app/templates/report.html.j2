<html>
  <body>
    <h2>{{ report_name }} ({{ report_id }})</h2>
    <p>Report date: {{ report_date }}</p>
    <h3>Key fields</h3>
    <ul>
      {% for key, value in fields.items() %}
      <li><strong>{{ key }}</strong>: {{ value }}</li>
      {% endfor %}
    </ul>
    <h3>Source URLs</h3>
    <ul>
      {% for url in urls %}
      <li><a href="{{ url }}">{{ url }}</a></li>
      {% endfor %}
    </ul>
  </body>
</html>
